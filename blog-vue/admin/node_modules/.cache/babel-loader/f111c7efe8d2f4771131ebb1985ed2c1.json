AA,CAAA,MAAA,CAAA,UAAA,EAAA,IAAA,CAAA,SAAA,CAAA,QAAA;AACA,cAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,YAAA,EAAA,KAAA,EAAA,IAAA,CAAA,gBAAA;AAAA,oBAAA,IAAA,QAAA,IAAA;;AACA,oBAAA,IAAA,CAAA,IAAA,EAAA;AACA;AACA,kBAAA,IAAA,CAAA,MAAA,CAAA,MAAA,CAAA,OAAA,EAAA,IAAA,CAAA,IAAA,EAFA,CAGA;;AACA,kBAAA,UAAA;AACA,kBAAA,IAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;AACA,kBAAA,IAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,oBAAA,IAAA,EAAA;AAAA,mBAAA;AACA,iBAPA,MAOA;AACA,kBAAA,IAAA,CAAA,QAAA,CAAA,KAAA,CAAA,IAAA,CAAA,OAAA;AACA;AACA,eAXA;AAYA;AACA,WArBA,CAAA,CAHA,CA0BA;;AACA,UAAA,OAAA,CAAA,IAAA;AACA,SA5BA,MA4BA;AACA,iBAAA,KAAA;AACA;AACA,OAhCA;AAiCA;AAnCA;AAfA,CAAA","sourcesContent":["<template>\n  <div class=\"login-container\">\n    <div class=\"login-card\">\n      <div class=\"login-title\">管理员登录</div>\n      <!-- 登录表单 -->\n      <el-form\n        status-icon\n        :model=\"loginForm\"\n        :rules=\"rules\"\n        ref=\"ruleForm\"\n        class=\"login-form\"\n      >\n        <!-- 用户名输入框 -->\n        <el-form-item prop=\"username\">\n          <el-input\n            v-model=\"loginForm.username\"\n            prefix-icon=\"el-icon-user-solid\"\n            placeholder=\"用户名\"\n            @keyup.enter.native=\"login\"\n          />\n        </el-form-item>\n        <!-- 密码输入框 -->\n        <el-form-item prop=\"password\">\n          <el-input\n            v-model=\"loginForm.password\"\n            prefix-icon=\"iconfont el-icon-mymima\"\n            show-password\n            placeholder=\"密码\"\n            @keyup.enter.native=\"login\"\n          />\n        </el-form-item>\n      </el-form>\n      <!-- 登录按钮 -->\n      <el-button type=\"primary\" @click=\"login\">登录</el-button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { generaMenu } from \"../../assets/js/menu\";\nexport default {\n  data: function() {\n    return {\n      loginForm: {\n        username: \"\",\n        password: \"\"\n      },\n      rules: {\n        username: [\n          { required: true, message: \"用户名不能为空\", trigger: \"blur\" }\n        ],\n        password: [{ required: true, message: \"密码不能为空\", trigger: \"blur\" }]\n      }\n    };\n  },\n  methods: {\n    login() {\n      this.$refs.ruleForm.validate(valid => {\n        if (valid) {\n          const that = this;\n          // eslint-disable-next-line no-undef\n          var captcha = new TencentCaptcha(\n            this.config.TENCENT_CAPTCHA,\n            function(res) {\n              if (res.ret === 0) {\n                //发送登录请求\n                let param = new URLSearchParams();\n                param.append(\"username\", that.loginForm.username);\n                param.append(\"password\", that.loginForm.password);\n                that.axios.post(\"/api/login\", param).then(({ data }) => {\n                  if (data.flag) {\n                    // 登录后保存用户信息\n                    that.$store.commit(\"login\", data.data);\n                    // 加载用户菜单\n                    generaMenu();\n                    that.$message.success(\"登录成功\");\n                    that.$router.push({ path: \"/\" });\n                  } else {\n                    that.$message.error(data.message);\n                  }\n                });\n              }\n            }\n          );\n          // 显示验证码\n          captcha.show();\n        } else {\n          return false;\n        }\n      });\n    }\n  }\n};\n</script>\n\n<style scoped>\n.login-container {\n  position: absolute;\n  top: 0;\n  bottom: 0;\n  right: 0;\n  left: 0;\n  /* todo 首页背景图 */\n  background: url(http://www.static.yehonghan.top/config/cover.jpg) center\n    center / cover no-repeat;\n}\n.login-card {\n  position: absolute;\n  top: 0;\n  bottom: 0;\n  right: 0;\n  background: #fff;\n  padding: 170px 60px 180px;\n  width: 350px;\n  opacity: 0.5;\n}\n.login-title {\n  color: #000;\n  font-weight: bold;\n  font-size: 2rem;\n}\n.login-form {\n  margin-top: 2rem;\n}\n.login-card button {\n  margin-top: 2rem;\n  width: 100%;\n}\n</style>\n"],"sourceRoot":"src/views/login"}]}